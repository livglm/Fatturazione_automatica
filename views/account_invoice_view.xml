<?xml version="1.0" encoding="utf-8"?><openerp><data><!--    Stock Picking Invoice Link module for Odoo    Copyright (C) 2013-2014 Akretion (http://www.akretion.com/)    @author: Alexis de Lattre <alexis.delattre@akretion.com>    The licence is in the file __openerp__.py--><template id="layout" inherit_id="web.layout">            <xpath expr="." positon="inside">                <link rel="stylesheet" href='/Fatturazione_automatica/static/src/css/remove_sheet.css'/>            </xpath></template>                <record id="invoice_form_pick" model="ir.ui.view">    <field name="name">related.pickings.account.invoice.sale.form</field>    <field name="model">account.invoice</field>    <field name="inherit_id" ref="account.invoice_form"/>    <field name="arch" type="xml">  <xpath expr="//form/sheet[@string='Invoice']" position="attributes">                    <attribute name="class">remove_sheet</attribute> </xpath>              <xpath expr="//button[@name='invoice_cance']" position="after">                    <button name="account_cancel" states="confirmed" string="Annulla Fattura" type="object" />                </xpath>	<field name="fiscal_position" position="after">		<field name="partner_invoice_id" />		<field name="partner_shipping_id"/>		<field name="immediate"/>	</field>                 <field name="invoice_line" position="after">							<group col='2'>                                 <field name="number_of_packages" string="Number of packages" />                                 <field name="credit"  />							</group>                 </field>       <notebook position="inside">            <page string="Shipping Informations" name="piede" > <!--attrs="{'invisible':[('immediate','!=',True)]}"-->			<group col='2'>				<group col='2'>						<field name="transportation_reason_id" placeholder='Seleziona....' />					<field name="goods_description_id" placeholder='Seleziona....'/>					<field name="carriage_condition_id" placeholder='Seleziona....' />				</group>					<group col='4'>					<field name="transportation_method_id" placeholder='Seleziona....'/>						<field name="dataora"/>				<group colspan="4"><!--				  <field name="carrier_id" domain="[('is_carrier', '=' ,True)]"/>-->				  <field name="carrier_id" />					</group> 				</group>           </group>             </page><!--            <page string="Pickings" name="pickings">                <field name="picking_ids"/>          </page>-->        </notebook>   </field></record>	<record id="account_invoice_imm" model="ir.ui.view">		<field name="name">account.invoice.sale.tree</field>		<field name="model">account.invoice</field>		<field name="inherit_id" ref="account.invoice_tree"/> 		<field name="arch" type="xml">			<field name="state" position="after">				<field name="immediate" context="{'immediate' : True}"/>			</field>		</field>	</record><!--          <record id="action_invoices_pick" model="ir.actions.act_window">            <field name="name">Fattura Accompagnatoria</field>            <field name="res_model">account.invoice</field>            <field name="view_type">form</field>            <field name="view_mode">tree,form,calendar,graph</field>            <field name="view_id"/>            <field name="domain">[('type','=','out_invoice'),('immediate', '=' ,True)]</field>            <field name="context">{'default_type':'out_invoice', 'type':'out_invoice', 'journal_type': 'sale','default_immediate' : True,'default_partner_shipping_id' : '','default_partner_invoice_id' : ''}</field>            <field name="search_view_id" ref="account.view_account_invoice_filter"/>            <field name="help" type="html">              <p class="oe_view_nocontent_create">                Click to create a customer invoice.              </p><p>                OpenERP's electronic invoicing allows to ease and fasten the                collection of customer payments. Your customer receives the                invoice by email and he can pay online and/or import it                in his own system.              </p><p>                The discussions with your customer are automatically displayed at                the bottom of each invoice.              </p>            </field>        </record>           <record id="action_invoices_tree_spec" model="ir.actions.act_window.view">            <field name="act_window_id" ref="action_invoices_pick"/>            <field name="view_id" ref="account.invoice_tree"/>            <field name="view_mode">tree</field>            <field name="sequence" eval="0"/>        </record>        <record id="action_invoices_form_spec" model="ir.actions.act_window.view">            <field name="act_window_id" ref="action_invoices_pick"/>            <field name="view_id" ref="account.invoice_form"/>            <field name="view_mode">form</field>            <field name="sequence" eval="1"/>        </record><menuitem id="menu_sub_sales_menuitem" parent="base.menu_sales" name="Fattura Accompagnatoria"  action="action_invoices_pick"/>          <record id="action_invoices_pick_f" model="ir.actions.act_window">            <field name="name">Fattura Accompagnatoria</field>            <field name="res_model">account.invoice</field>            <field name="view_type">form</field>            <field name="view_mode">tree,form,calendar,graph</field>            <field name="view_id"/>            <field name="domain">[('type','=','in_invoice'),('immediate', '=' ,True)]</field>            <field name="context">{'default_type':'in_invoice', 'type':'in_invoice', 'journal_type': 'purchase','default_immediate' : True}</field>            <field name="search_view_id" ref="account.view_account_invoice_filter"/>            <field name="help" type="html">              <p class="oe_view_nocontent_create">                Click to create a customer invoice.              </p><p>                OpenERP's electronic invoicing allows to ease and fasten the                collection of customer payments. Your customer receives the                invoice by email and he can pay online and/or import it                in his own system.              </p><p>                The discussions with your customer are automatically displayed at                the bottom of each invoice.              </p>            </field>        </record>            <record id="action_invoices_tree_spec" model="ir.actions.act_window.view">            <field name="act_window_id" ref="action_invoices_pick_f"/>            <field name="view_id" ref="account.invoice_tree"/>            <field name="view_mode">tree</field>            <field name="sequence" eval="0"/>        </record>        <record id="action_invoices_form_spec" model="ir.actions.act_window.view">            <field name="act_window_id" ref="action_invoices_pick_f"/>            <field name="view_id" ref="account.invoice_form"/>            <field name="view_mode">form</field>            <field name="sequence" eval="1"/>        </record><menuitem id="menu_sub_purchase_menuitem" parent="base.menu_sales" name="Fattura Accompagnatoria2"  action="action_invoices_pick_f"/>--></data></openerp>